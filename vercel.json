{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.12"
      }
    },
    {
      "src": "api/**/*.rs",
      "use": "vercel-rust@4.0.7"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/\$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ]
}
